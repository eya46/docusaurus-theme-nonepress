{
  "name": "@docusaurus/theme-classic",
  "version": "2.0.0-beta.4",
  "description": "Classic theme for Docusaurus",
  "main": "lib/index.js",
  "types": "src/types.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/docusaurus.git",
    "directory": "packages/docusaurus-theme-classic"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit && yarn babel && yarn prettier",
    "babel": "cross-env BABEL_ENV=lib babel src -d lib --extensions \".tsx,.ts\" --ignore \"**/*.d.ts\" --copy-files",
    "prettier": "prettier --write \"**/*.{js,ts,jsx,tsc}\"",
    "update-code-translations": "node -e 'require(\"./update-code-translations.js\").run()'"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.4",
    "@docusaurus/plugin-content-docs": "^2.0.0-beta.4",
    "@docusaurus/plugin-content-pages": "^2.0.0-beta.4",
    "@docusaurus/types": "^2.0.0-beta.4",
    "@docusaurus/utils": "^2.0.0-beta.4",
    "@docusaurus/utils-common": "^2.0.0-beta.4",
    "@docusaurus/utils-validation": "^2.0.0-beta.4",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "autoprefixer": "^10.3.1",
    "chalk": "^4.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.2",
    "lodash": "^4.17.20",
    "postcss": "^8.2.15",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.23.0",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.4",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.15",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0",
    "react-dom": "^16.8.4 || ^17.0.0"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
